ecciones\r\n        return index % 2 !== 0;\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Estilos generales para la página */\r\n  .recipe-page {\r\n    background-color: #000; /* Fondo negro */\r\n    color: white;\r\n    padding: 20px;\r\n  }\r\n  \r\n  /* Estilo para cada sección de recetas */\r\n  .recipe-section {\r\n    margin: 40px 0;\r\n  }\r\n  \r\n  /* Estilos para las secciones normales */\r\n  .recipe-content {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    flex-wrap: wrap;\r\n    gap: 20px;\r\n  }\r\n  \r\n  /* Estilos para las secciones alternadas */\r\n  .recipe-section.reverse .recipe-content {\r\n    flex-direction: row-reverse; /* Cambia el orden para las secciones alternadas */\r\n  }\r\n  \r\n  .recipe-image {\r\n    width: 50%;\r\n    max-width: 500px;\r\n    border-radius: 10%;\r\n    padding-right: 10;\r\n  }\r\n  \r\n  .recipe-info {\r\n    width: 45%;\r\n    text-align: left;\r\n  }\r\n  \r\n  .recipe-info h2 {\r\n    font-size: 32px;\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n  .recipe-info p {\r\n    font-size: 18px;\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  .recipe-info button {\r\n    padding: 10px 20px;\r\n    background-color: #a80000;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .recipe-info button:hover {\r\n    background-color: #f44336;\r\n  }\r\n  </style>\r\n  "],"mappings":";AA6EE,eAAe;EACbA,OAAO,EAAE;IACPC,WAAWA,CAACC,QAAQ,EAAE;MACpB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,YAAYF,QAAQ,EAAE,CAAC,EAAE;IAC7C,CAAC;IACDG,KAAKA,CAACC,KAAK,EAAE;MACX;MACA,OAAOA,KAAI,GAAI,MAAM,CAAC;IACxB;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}