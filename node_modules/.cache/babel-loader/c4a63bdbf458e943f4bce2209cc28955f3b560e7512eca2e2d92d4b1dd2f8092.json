x-width: 600px;\r\n  box-shadow: 0px 4px 20px rgba(255, 255, 255, 0.2);\r\n}\r\n\r\n.cta-title {\r\n  font-size: 36px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.cta-card h2 {\r\n  font-size: 24px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.cta-card p {\r\n  font-size: 16px;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.cta-card button {\r\n  background-color: #a80000;\r\n  color: white;\r\n  padding: 12px 25px;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.cta-card button:hover {\r\n  background-color: #800000;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n/* Contenedor de imágenes de recetas */\r\n.recipes-images-container {\r\n  margin-top: 20px;\r\n  background-color: rgba(0, 0, 0, 0.6); /* Fondo oscuro semitransparente */\r\n  padding: 20px;\r\n  border-radius: 15px;\r\n  box-shadow: 0px 4px 20px rgba(255, 255, 255, 0.2);\r\n  max-width: 800px;\r\n}\r\n\r\n.recipes-images-container h3 {\r\n  margin-bottom: 15px;\r\n  font-size: 24px;\r\n}\r\n\r\n.recipes-images {\r\n  display: flex;\r\n  justify-content: center;\r\n  gap: 10px;\r\n}\r\n\r\n.recipes-images img {\r\n  width: 150px;\r\n  height: 150px;\r\n  object-fit: cover;\r\n  border-radius: 10px;\r\n  transition: transform 0.3s;\r\n}\r\n\r\n.recipes-images img:hover {\r\n  transform: scale(1.1);\r\n}\r\n</style>\r\n"],"mappings":";OAKgBA,UAAoC;;EAJ7CC,KAAK,EAAC;AAAgB;;EAUpBA,KAAK,EAAC;AAAiB;;EAErBA,KAAK,EAAC;AAAU;;uBAZzBC,mBAAA,CA8BM,OA9BNC,UA8BM,GA7BJC,mBAAA,oBAAuB,E,0BACvBC,mBAAA,CAKM;IALDJ,KAAK,EAAC;EAA4B,IACrCI,mBAAA,CAGQ;IAHDC,QAAQ,EAAR,EAAQ;IAACC,KAAK,EAAL,EAAK;IAACC,IAAI,EAAJ;MACpBH,mBAAA,CAA8D;IAAtDI,GAAoC,EAApCT,UAAoC;IAACU,IAAI,EAAC;MAL1DC,gBAAA,CAKsE,iDAEhE,E,wBAGFP,mBAAA,4CAA+C,EAC/CC,mBAAA,CAmBM,OAnBNO,UAmBM,GAlBJR,mBAAA,iBAAoB,EACpBC,mBAAA,CAKM,OALNQ,UAKM,G,0BAJJR,mBAAA,CAAiD;IAA7CJ,KAAK,EAAC;EAAW,GAAC,wBAAsB,sB,0BAC5CI,mBAAA,CAA6D,YAAzD,sDAAoD,sB,0BACxDA,mBAAA,CAA+I,WAA5I,0IAAwI,sBAC3IA,mBAAA,CAAuD;IAA9CS,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,YAAA,IAAAD,