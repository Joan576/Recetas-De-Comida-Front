{"ast":null,"code":"import { ref, onMounted } from 'vue';\nexport default {\n  __name: 'UserProfile',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const form = ref({\n      username: '',\n      description: ''\n    });\n    const errorMessage = ref('');\n    onMounted(async () => {\n      const userId = localStorage.getItem('userId'); // Obtén el userId de localStorage\n      if (!userId) {\n        errorMessage.value = 'Usuario no autenticado';\n        return;\n      }\n      try {\n        const response = await fetch(`http://localhost:4000/api/profile/${userId}`, {\n          method: 'GET',\n          headers: {\n            'Content-Type': 'application/json'\n          }\n        });\n        const data = await response.json();\n        if (response.ok) {\n          form.value.username = data.nombre_usuario;\n          form.value.description = data.descripcion || '';\n        } else {\n          errorMessage.value = data.message || 'Error al cargar los datos del perfil';\n        }\n      } catch (error) {\n        errorMessage.value = 'Error en el servidor: ' + error.message;\n      }\n    });\n    const __returned__ = {\n      form,\n      errorMessage,\n      ref,\n      onMounted\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","form","username","description","errorMessage","userId","localStorage","getItem","value","response","fetch","method","headers","data","json","ok","nombre_usuario","descripcion","message","error"],"sources":["C:/Users/MEZAS/OneDrive/Escritorio/Trabajos U/Programacion Web/Proyecto_Recetas/recetas_front/src/components/UserProfile.vue"],"sourcesContent":["<template>\r\n  <div class=\"profile-page\">\r\n    <h2>Perfil de Usuario</h2>\r\n\r\n    <form @submit.prevent=\"handleUpdate\">\r\n      <div class=\"input-group\">\r\n        <label for=\"username\">Nombre de Usuario:</label>\r\n        <input type=\"text\" id=\"username\" v-model=\"form.username\" required />\r\n      </div>\r\n\r\n      <div class=\"input-group\">\r\n        <label for=\"description\">Descripción:</label>\r\n        <textarea id=\"description\" v-model=\"form.description\" placeholder=\"Añade una breve descripción de ti...\"></textarea>\r\n      </div>\r\n\r\n      <button type=\"submit\">Guardar Cambios</button>\r\n\r\n      <p v-if=\"successMessage\" class=\"success\">{{ successMessage }}</p>\r\n      <p v-if=\"errorMessage\" class=\"error\">{{ errorMessage }}</p>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted } from 'vue';\r\n\r\nconst form = ref({\r\n  username: '',\r\n  description: '',\r\n});\r\n\r\nconst errorMessage = ref('');\r\n\r\nonMounted(async () => {\r\n  const userId = localStorage.getItem('userId'); // Obtén el userId de localStorage\r\n  if (!userId) {\r\n    errorMessage.value = 'Usuario no autenticado';\r\n    return;\r\n  }\r\n\r\n  try {\r\n    const response = await fetch(`http://localhost:4000/api/profile/${userId}`, {\r\n      method: 'GET',\r\n      headers: {\r\n        'Content-Type': 'application/json',\r\n      },\r\n    });\r\n\r\n    const data = await response.json();\r\n    if (response.ok) {\r\n      form.value.username = data.nombre_usuario;\r\n      form.value.description = data.descripcion || '';\r\n    } else {\r\n      errorMessage.value = data.message || 'Error al cargar los datos del perfil';\r\n    }\r\n  } catch (error) {\r\n    errorMessage.value = 'Error en el servidor: ' + error.message;\r\n  }\r\n});\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.profile-page {\r\n  max-width: 500px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 10px;\r\n  background-color: #f9f9f9;\r\n}\r\n\r\nh2 {\r\n  text-align: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.input-group {\r\n  margin-bottom: 15px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  font-weight: bold;\r\n  margin-bottom: 5px;\r\n}\r\n\r\ninput[type=\"text\"],\r\ntextarea {\r\n  width: 100%;\r\n  padding: 10px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px;\r\n}\r\n\r\nbutton {\r\n  width: 100%;\r\n  padding: 10px;\r\n  background-color: #007BFF;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  font-size: 16px;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n.error {\r\n  color: red;\r\n  margin-top: 10px;\r\n  font-size: 14px;\r\n}\r\n\r\n.success {\r\n  color: green;\r\n  margin-top: 10px;\r\n  font-size: 14px;\r\n}\r\n</style>\r\n"],"mappings":"AAwBA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;;;;;;;IAEpC,MAAMC,IAAI,GAAGF,GAAG,CAAC;MACfG,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE;IACf,CAAC,CAAC;IAEF,MAAMC,YAAY,GAAGL,GAAG,CAAC,EAAE,CAAC;IAE5BC,SAAS,CAAC,YAAY;MACpB,MAAMK,MAAM,GAAGC,YAAY,CAACC,OAAO,CAAC,QAAQ,CAAC,CAAC,CAAC;MAC/C,IAAI,CAACF,MAAM,EAAE;QACXD,YAAY,CAACI,KAAK,GAAG,wBAAwB;QAC7C;MACF;MAEA,IAAI;QACF,MAAMC,QAAQ,GAAG,MAAMC,KAAK,CAAC,qCAAqCL,MAAM,EAAE,EAAE;UAC1EM,MAAM,EAAE,KAAK;UACbC,OAAO,EAAE;YACP,cAAc,EAAE;UAClB;QACF,CAAC,CAAC;QAEF,MAAMC,IAAI,GAAG,MAAMJ,QAAQ,CAACK,IAAI,CAAC,CAAC;QAClC,IAAIL,QAAQ,CAACM,EAAE,EAAE;UACfd,IAAI,CAACO,KAAK,CAACN,QAAQ,GAAGW,IAAI,CAACG,cAAc;UACzCf,IAAI,CAACO,KAAK,CAACL,WAAW,GAAGU,IAAI,CAACI,WAAW,IAAI,EAAE;QACjD,CAAC,MAAM;UACLb,YAAY,CAACI,KAAK,GAAGK,IAAI,CAACK,OAAO,IAAI,sCAAsC;QAC7E;MACF,CAAC,CAAC,OAAOC,KAAK,EAAE;QACdf,YAAY,CAACI,KAAK,GAAG,wBAAwB,GAAGW,KAAK,CAACD,OAAO;MAC/D;IACF,CAAC,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}