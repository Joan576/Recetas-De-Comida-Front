00;\r\n  color: #ffffff;\r\n  display: flex;\r\n  flex-direction: column;\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\n/* Estilos para la sección CTA */\r\n.cta {\r\n  position: relative;\r\n  min-height: 100vh;\r\n  background-image: url('https://img.freepik.com/foto-gratis/fondo-abstracto-borroso-cocina-moderna-mesa-espacio-exhibir-sus-productos_91908-1263.jpg?size=626&ext=jpg');\r\n  background-size: cover;\r\n  background-position: center;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n/* Estilos para la tarjeta dentro de la sección CTA */\r\n.cta-card {\r\n  background-color: rgba(255, 255, 255, 0.01); /* Fondo blanco semitransparente */\r\n  padding: 30px;\r\n  border-radius: 15px;\r\n  box-shadow: 0px 4px 20px rgba(0, 0, 0, 0.3);\r\n  max-width: 600px;\r\n  text-align: center;\r\n}\r\n\r\n.cta-card h2 {\r\n  color: #333;\r\n  font-size: 36px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.cta-card p {\r\n  color: #555;\r\n  font-size: 18px;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.cta-card button {\r\n  background-color: #a80000;\r\n  color: #fff;\r\n  padding: 12px 25px;\r\n  font-size: 20px;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.cta-card button:hover {\r\n  background-color: #800000;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n/* Estilos para la sección de recetas */\r\n.recipes-section {\r\n  padding: 40px 20px;\r\n  background-color: #000; /* Fondo negro */\r\n  text-align: center;\r\n}\r\n\r\n.recipes-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fill, minmax(250px, 1fr)); /* Ajusta a 4 columnas */\r\n  gap: 20px;\r\n  justify-content: center; /* Centrar las tarjetas en la sección */\r\n  max-width: 1200px; /* Limitar el ancho máximo del grid */\r\n  margin: 0 auto; /* Centrar el grid en la página */\r\n}\r\n\r\n/* Estilos para el contenedor del video */\r\n.video-container {\r\n  margin: 20px;\r\n}\r\n\r\n.video-container h3 