padding: 0;\r\n  height: 100%;\r\n  font-family: Arial, sans-serif;\r\n  background-color: #000;\r\n}\r\n\r\n/* Contenedor principal */\r\n.home-container {\r\n  max-width: 100vw;\r\n  min-height: 100vh;\r\n  background-color: #000;\r\n  color: #fff;\r\n  display: flex;\r\n  flex-direction: column;\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.cta {\r\n  position: relative;\r\n  min-height: 100vh;\r\n  background-image: url('https://images.pexels.com/photos/1640773/pexels-photo-1640773.jpeg');\r\n  background-size: cover;\r\n  background-position: center;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  margin-top: 0px;\r\n  margin-bottom: 0px;\r\n  z-index: 1;\r\n}\r\n\r\n/* Agregar una capa oscura sobre la imagen de fondo */\r\n.cta::before {\r\n  content: '';\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.2); /* Ajusta la opacidad con el último valor */\r\n  z-index: -1; /* Envía la capa detrás del contenido */\r\n}\r\n\r\n\r\n/* Estilos del título */\r\n.cta-title {\r\n  font-size: 48px;\r\n  color: white;\r\n  text-shadow: 2px 2px 5px rgba(0, 0, 0, 0.8); /* Sombra negra suave */\r\n  margin-bottom: 30px;\r\n  z-index: 1; /* Asegura que el título esté por encima de la tarjeta */\r\n}\r\n\r\n/* Estilos de la tarjeta */\r\n.cta-card {\r\n  background-color: rgba(0, 0, 0, 0.6); /* Fondo oscuro semitransparente */\r\n  padding: 30px;\r\n  border-radius: 15px;\r\n  box-shadow: 0px 4px 20px rgba(255, 255, 255, 0.3);\r\n  max-width: 900px;\r\n  text-align: center;\r\n  z-index: 1; /* Asegura que esté sobre el fondo */\r\n}\r\n\r\n.cta-card h2 {\r\n  color: #ffffff;\r\n  font-size: 36px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.cta-card p {\r\n  color: #ffffff;\r\n  font-size: 18px;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.cta-card button {\r\n  background-color: #a80000;\r\n  color: #fff;\r\n  padding: 12px 25px;\r\n  font-size: 20px;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.cta-card button:hover {\r\n  background-color: #800000;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n/* Contenedor del video */\r\n.video-container {\r\n  width: 100%;\r\n  height: 100vh; /* Altura completa del viewport */\r\n  position: relative;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  margin-bottom: 60px;\r\n}\r\n\r\n/* Tarjeta sobre el video */\r\n.video-overlay {\r\n  position: absolute;\r\n  background-color: rgba(0, 0, 0, 0.6); /* Fondo oscuro semitransparente */\r\n  color: #fff;\r\n  padding: 30px;\r\n  border-radius: 15px;\r\n  text-align: center;\r\n  z-index: 1;\r\n  max-width: 600px;\r\n}\r\n\r\n.video-overlay h2 {\r\n  font-size: 36px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.video-overlay p {\r\n  font-size: 18px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.video-overlay button {\r\n  background-color: #a80000;\r\n  color: #fff;\r\n  padding: 12px 25px;\r\n  font-size: 20px;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.video-overlay button:hover {\r\n  background-color: #800000;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n/* Estilos para el video */\r\nvideo {\r\n  width: 100%;\r\n  height: auto;\r\n  object-fit: cover;\r\n}\r\n\r\nh2, p {\r\n  text-shadow: 2px 2px 5px rgba(0, 0, 0, 0.8); /* Sombra negra suave */\r\n}\r\n</style>\r\n"],"mappings":";OAQgBA,UAAoC;;EAP7CC,KAAK,EAAC;AAAgB;;EAYdA,KAAK,EAAC;AAAK;;EAEfA,KAAK,EAAC;AAAU;;uBAdzBC,mBAAA,CAqBM,OArBNC,UAqBM,GApBJC,mBAAA,sDAAyD,E,0BACzDC,mBAAA,CAQM;IARDJ,KAAK,EAAC;EAAiB,IAC1BI,mBAAA,CAEM;IAFDJ,KAAK,EAAC;EAAe,IAG1BI,mBAAA,CAGQ;IAHDC,KAAK,EAAC,MAAM;IAACC,MAAM,EAAC,MAAM;IAACC,KAAK,EAAL,EAAK;IAACC,QAAQ,EAAR,EAAQ;IAACC,IAAI,EAAJ;MAC/CL,mBAAA,CAA8D;IAAtDM,GAAoC,EAApCX,UAAoC;IAACY,IAAI,EAAC;MAR1DC,gBAAA,CAQsE,iDAEhE,E,wBAEAT,mBAAA,0DAA6D,EAC7DC,mBAAA,CAQQ,WARRS,UAQQ,GAPRV,mBAAA,mCAAsC,EACtCC,mBAAA,CAKM,OALNU,UAKM,G,0