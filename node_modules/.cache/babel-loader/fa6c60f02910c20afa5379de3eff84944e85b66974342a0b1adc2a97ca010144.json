{"ast":null,"code":"import \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport { ref, onMounted } from 'vue';\nconst defaultProfileImage = 'ruta/a/imagen/default.png'; // Cambia esto por una imagen por defecto\n\nexport default {\n  __name: 'UserProfile',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const form = ref({\n      username: '',\n      description: '',\n      imagen_perfil: null\n    });\n    const profileImageUrl = ref(null); // URL de la imagen actual de perfil\n    const errorMessage = ref('');\n    const successMessage = ref('');\n\n    // Obtener el userId desde localStorage\n    const userId = JSON.parse(localStorage.getItem('user'))?.id || '1';\n    onMounted(async () => {\n      try {\n        const response = await fetch(`http://localhost:4000/api/profile/${userId}`);\n        const data = await response.json();\n        if (response.ok) {\n          form.value.username = data.nombre_usuario;\n          form.value.description = data.descripcion || '';\n          profileImageUrl.value = data.imagen_perfil || defaultProfileImage;\n        } else {\n          errorMessage.value = data.message || 'Error al cargar el perfil';\n        }\n      } catch (error) {\n        errorMessage.value = 'Error en el servidor: ' + error.message;\n      }\n    });\n    const onFileChange = e => {\n      const file = e.target.files[0];\n      if (file) {\n        form.value.imagen_perfil = file;\n        profileImageUrl.value = URL.createObjectURL(file); // Actualiza la vista previa de la imagen\n      }\n    };\n    const triggerFileInput = () => {\n      // Simula un clic en el input de archivo para cambiar la imagen de perfil\n      document.getElementById(\"imagen_perfil\").click();\n    };\n    const handleUpdate = async () => {\n      const formData = new FormData();\n      formData.append('username', form.value.username);\n      formData.append('description', form.value.description);\n      if (form.value.imagen_perfil) {\n        formData.append('imagen_perfil', form.value.imagen_perfil);\n      }\n      try {\n        const response = await fetch(`http://localhost:4000/api/profile/${userId}`, {\n          method: 'PUT',\n          body: formData\n        });\n        const data = await response.json();\n        if (response.ok) {\n          successMessage.value = 'Perfil actualizado con éxito';\n          errorMessage.value = '';\n        } else {\n          errorMessage.value = data.message || 'Error al actualizar el perfil';\n          successMessage.value = '';\n        }\n      } catch (error) {\n        errorMessage.value = 'Error en el servidor: ' + error.message;\n        successMessage.value = '';\n      }\n    };\n    const __returned__ = {\n      form,\n      profileImageUrl,\n      defaultProfileImage,\n      errorMessage,\n      successMessage,\n      userId,\n      onFileChange,\n      triggerFileInput,\n      handleUpdate,\n      ref,\n      onMounted\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","defaultProfileImage","form","username","description","imagen_perfil","profileImageUrl","errorMessage","successMessage","userId","JSON","parse","localStorage","getItem","id","response","fetch","data","json","ok","value","nombre_usuario","descripcion","message","error","onFileChange","e","file","target","files","URL","createObjectURL","triggerFileInput","document","getElementById","click","handleUpdate","formData","FormData","append","method","body"],"sources":["C:/Users/MEZAS/OneDrive/Escritorio/Trabajos U/Programacion Web/Proyecto_Recetas/recetas_front/src/components/UserProfile.vue"],"sourcesContent":["<template>\r\n  <div class=\"profile-page\">\r\n    <h2>Perfil de Usuario</h2>\r\n\r\n    <!-- Imagen de perfil con opción de cambiarla al hacer clic -->\r\n    <div class=\"profile-image-container\">\r\n      <label for=\"imagen_perfil\">\r\n        <img\r\n          :src=\"profileImageUrl || defaultProfileImage\"\r\n          alt=\"Imagen de perfil\"\r\n          class=\"profile-image\"\r\n          @click=\"triggerFileInput\"\r\n        />\r\n      </label>\r\n      <input type=\"file\" id=\"imagen_perfil\" @change=\"onFileChange\" ref=\"fileInput\" style=\"display: none\" />\r\n    </div>\r\n\r\n    <form @submit.prevent=\"handleUpdate\">\r\n      <div class=\"input-group\">\r\n        <label for=\"username\">Nombre de Usuario:</label>\r\n        <input type=\"text\" id=\"username\" v-model=\"form.username\" required />\r\n      </div>\r\n\r\n      <div class=\"input-group\">\r\n        <label for=\"description\">Descripción:</label>\r\n        <textarea id=\"description\" v-model=\"form.description\" placeholder=\"Añade una breve descripción de ti...\"></textarea>\r\n      </div>\r\n\r\n      <button type=\"submit\">Guardar Cambios</button>\r\n\r\n      <p v-if=\"successMessage\" class=\"success\">{{ successMessage }}</p>\r\n      <p v-if=\"errorMessage\" class=\"error\">{{ errorMessage }}</p>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted } from 'vue';\r\n\r\nconst form = ref({\r\n  username: '',\r\n  description: '',\r\n  imagen_perfil: null,\r\n});\r\nconst profileImageUrl = ref(null); // URL de la imagen actual de perfil\r\nconst defaultProfileImage = 'ruta/a/imagen/default.png'; // Cambia esto por una imagen por defecto\r\nconst errorMessage = ref('');\r\nconst successMessage = ref('');\r\n\r\n// Obtener el userId desde localStorage\r\nconst userId = JSON.parse(localStorage.getItem('user'))?.id || '1';\r\n\r\nonMounted(async () => {\r\n  try {\r\n    const response = await fetch(`http://localhost:4000/api/profile/${userId}`);\r\n    const data = await response.json();\r\n\r\n    if (response.ok) {\r\n      form.value.username = data.nombre_usuario;\r\n      form.value.description = data.descripcion || '';\r\n      profileImageUrl.value = data.imagen_perfil || defaultProfileImage;\r\n    } else {\r\n      errorMessage.value = data.message || 'Error al cargar el perfil';\r\n    }\r\n  } catch (error) {\r\n    errorMessage.value = 'Error en el servidor: ' + error.message;\r\n  }\r\n});\r\n\r\nconst onFileChange = (e) => {\r\n  const file = e.target.files[0];\r\n  if (file) {\r\n    form.value.imagen_perfil = file;\r\n    profileImageUrl.value = URL.createObjectURL(file); // Actualiza la vista previa de la imagen\r\n  }\r\n};\r\n\r\nconst triggerFileInput = () => {\r\n  // Simula un clic en el input de archivo para cambiar la imagen de perfil\r\n  document.getElementById(\"imagen_perfil\").click();\r\n};\r\n\r\nconst handleUpdate = async () => {\r\n  const formData = new FormData();\r\n  formData.append('username', form.value.username);\r\n  formData.append('description', form.value.description);\r\n  if (form.value.imagen_perfil) {\r\n    formData.append('imagen_perfil', form.value.imagen_perfil);\r\n  }\r\n\r\n  try {\r\n    const response = await fetch(`http://localhost:4000/api/profile/${userId}`, {\r\n      method: 'PUT',\r\n      body: formData,\r\n    });\r\n\r\n    const data = await response.json();\r\n    if (response.ok) {\r\n      successMessage.value = 'Perfil actualizado con éxito';\r\n      errorMessage.value = '';\r\n    } else {\r\n      errorMessage.value = data.message || 'Error al actualizar el perfil';\r\n      successMessage.value = '';\r\n    }\r\n  } catch (error) {\r\n    errorMessage.value = 'Error en el servidor: ' + error.message;\r\n    successMessage.value = '';\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.profile-page {\r\n  max-width: 500px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 10px;\r\n  background-color: #f9f9f9;\r\n}\r\n\r\nh2 {\r\n  text-align: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.profile-image-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.profile-image {\r\n  width: 120px;\r\n  height: 120px;\r\n  border-radius: 50%;\r\n  cursor: pointer;\r\n  object-fit: cover;\r\n  border: 2px solid #ccc;\r\n}\r\n\r\n.input-group {\r\n  margin-bottom: 15px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  font-weight: bold;\r\n  margin-bottom: 5px;\r\n}\r\n\r\ninput[type=\"text\"],\r\ntextarea {\r\n  width: 100%;\r\n  padding: 10px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px;\r\n}\r\n\r\nbutton {\r\n  width: 100%;\r\n  padding: 10px;\r\n  background-color: #007BFF;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  font-size: 16px;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n.error {\r\n  color: red;\r\n  margin-top: 10px;\r\n  font-size: 14px;\r\n}\r\n\r\n.success {\r\n  color: green;\r\n  margin-top: 10px;\r\n  font-size: 14px;\r\n}\r\n</style>\r\n"],"mappings":";;;AAqCA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AAQpC,MAAMC,mBAAmB,GAAG,2BAA2B,CAAC,CAAC;;;;;;;;IANzD,MAAMC,IAAI,GAAGH,GAAG,CAAC;MACfI,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE,EAAE;MACfC,aAAa,EAAE;IACjB,CAAC,CAAC;IACF,MAAMC,eAAe,GAAGP,GAAG,CAAC,IAAI,CAAC,CAAC,CAAC;IAEnC,MAAMQ,YAAY,GAAGR,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMS,cAAc,GAAGT,GAAG,CAAC,EAAE,CAAC;;IAE9B;IACA,MAAMU,MAAM,GAAGC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,CAAC,EAAEC,EAAE,IAAI,GAAG;IAElEd,SAAS,CAAC,YAAY;MACpB,IAAI;QACF,MAAMe,QAAQ,GAAG,MAAMC,KAAK,CAAC,qCAAqCP,MAAM,EAAE,CAAC;QAC3E,MAAMQ,IAAI,GAAG,MAAMF,QAAQ,CAACG,IAAI,CAAC,CAAC;QAElC,IAAIH,QAAQ,CAACI,EAAE,EAAE;UACfjB,IAAI,CAACkB,KAAK,CAACjB,QAAQ,GAAGc,IAAI,CAACI,cAAc;UACzCnB,IAAI,CAACkB,KAAK,CAAChB,WAAW,GAAGa,IAAI,CAACK,WAAW,IAAI,EAAE;UAC/ChB,eAAe,CAACc,KAAK,GAAGH,IAAI,CAACZ,aAAa,IAAIJ,mBAAmB;QACnE,CAAC,MAAM;UACLM,YAAY,CAACa,KAAK,GAAGH,IAAI,CAACM,OAAO,IAAI,2BAA2B;QAClE;MACF,CAAC,CAAC,OAAOC,KAAK,EAAE;QACdjB,YAAY,CAACa,KAAK